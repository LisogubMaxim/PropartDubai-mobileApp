{
    "expo": {
        "name": "propart-dubai-react-native",
        "slug": "propart-dubai-react-native",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            [
                "expo-font",
                {
                    "fonts": [
                        "./assets/fonts/Nunito/Nunito-Bold.ttf",
                        "./assets/fonts/Nunito/Nunito-Medium.ttf",
                        "./assets/fonts/Nunito/Nunito-Regular.ttf",
                        "./assets/fonts/OpenSans/OpenSans-SemiBold.ttf"
                    ]
                }
            ]
        ]
    }
}
